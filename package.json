{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ghq",
  "title": "ghq",
  "description": "",
  "icon": "command-icon.png",
  "author": "r_takaishi",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show ghq list",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.2",
    "@raycast/utils": "^1.3.2",
    "shelljs": "^0.8.5",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "preferences": [
    {
      "required": false,
      "name": "path",
      "title": "PATH",
      "type": "textfield",
      "description": "PATH",
      "placeholder": "/path/to/bin"
    },
    {
      "required": false,
      "name": "configPath",
      "title": "Config Path",
      "type": "textfield",
      "description": "Config file path",
      "placeholder": "/path/to/config.json",
      "default": "~/.raycast_ghq.json"
    }
  ]
}
